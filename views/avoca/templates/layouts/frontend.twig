<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,300i,400,400i,500,500i,600,600i,700,700i|Playfair+Display:400,400i,700,700i,900,900i" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.1/css/all.css" integrity="sha384-gfdkjb5BdAXd+lj+gudLWI+BXq4IuLW5IT+brZEZsLFm++aCMlF1V92rMkPaX4PP" crossorigin="anonymous">
    <!-- Bootstrap CSS File -->
    <link href="{{ avoca_static() }}/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- Libraries CSS Files -->
    <link href="{{ avoca_static() }}/lib/ionicons/css/ionicons.min.css" rel="stylesheet">
    <link href="{{ avoca_static() }}/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="{{ avoca_static() }}/lib/magnific-popup/magnific-popup.css" rel="stylesheet">
    <link href="{{ avoca_static() }}/lib/hover/hover.min.css" rel="stylesheet">

    <!-- Main Stylesheet File -->
    <link rel="stylesheet" href="{{ avoca_static() }}/css/style.css">
    <!-- Responsive css -->
    <link href="{{ avoca_static() }}/css/responsive.css" rel="stylesheet">
    <!-- Favicon -->
    <link rel="shortcut icon" href="{{ avoca_static() }}/images/favicon.png">

    <title>{{ _pageTitle }}</title>
    <meta content="" name="keywords">
    <meta content="" name="description">
</head>
<body>

<!-- start section navbar -->
<nav id="main-nav">
    <div class="row">
        <div class="container">

            <div class="logo">
                <a href="index.html"><img src="{{ avoca_static() }}/images/logo.png" alt="logo"></a>
            </div>

            <div class="responsive"><i data-icon="m" class="ion-navicon-round"></i></div>

            <ul class="nav-menu list-unstyled">
                <li><a href="#header" class="smoothScroll">Home</a></li>
                <li><a href="#about" class="smoothScroll">About</a></li>
                <li><a href="#portfolio" class="smoothScroll">Portfolio</a></li>
                <li><a href="#experience" class="smoothScroll">Experience</a></li>
                <li><a href="#journal" class="smoothScroll">Blog</a></li>
                <li><a href="#contact" class="smoothScroll">Contact</a></li>
            </ul>

        </div>
    </div>
</nav>
<!-- End section navbar -->

{% block content %}{% endblock %}

<!-- start section footer -->
<div id="footer" class="text-center">
    <div class="container">
        <div class="socials-media text-center">

            <ul class="list-unstyled">
                <li><a href="http://avoca.io" target="_blank"><i class="fab fa-internet-explorer"></i></a></li>
                <li><a href="https://github.com/AvocaPlatform/Avoca-Profile" target="_blank"><i class="ion-social-github"></i></a></li>
            </ul>

        </div>

        <p>&copy; Copyrights AvocaIO. All rights reserved.</p>

        <div class="credits">
            Designed by <a href="https://bootstrapmade.com/" target="_blank">BootstrapMade</a>
        </div>

    </div>
</div>
<!-- End section footer -->

<!-- JavaScript Libraries -->
<script src="{{ avoca_static() }}/lib/jquery/jquery.min.js"></script>
<script src="{{ avoca_static() }}/lib/jquery/jquery-migrate.min.js"></script>
<script src="{{ avoca_static() }}/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="{{ avoca_static() }}/lib/typed/typed.js"></script>
<script src="{{ avoca_static() }}/lib/owlcarousel/owl.carousel.min.js"></script>
<script src="{{ avoca_static() }}/lib/magnific-popup/magnific-popup.min.js"></script>
<script src="{{ avoca_static() }}/lib/isotope/isotope.pkgd.min.js"></script>
<!-- Contact Form JavaScript File -->
<script src="{{ avoca_static() }}/contactform/contactform.js"></script>
<!-- Template Main Javascript File -->
<script src="{{ avoca_static() }}/js/main.js"></script>
</body>
</html>